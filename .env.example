# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# API Configuration
API_PREFIX=/api/v1
API_DESCRIPTION=A modular Fastify TypeScript REST API

# Application
APP_NAME=Fastify TS API
APP_VERSION=1.0.0

# Pagination
PAGINATION_DEFAULT_PAGE=1
PAGINATION_DEFAULT_LIMIT=10
PAGINATION_MAX_LIMIT=100

# Data
ENABLE_SAMPLE_DATA=true